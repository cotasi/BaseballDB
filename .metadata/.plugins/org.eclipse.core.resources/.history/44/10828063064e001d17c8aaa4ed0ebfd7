package dto;

import java.io.Serializable;


public class Product implements Serializable {
	
	private static final long serialVersionUID = -4274700572038677000L;
	
	
	private String pname; // 상품명
	private String pename; // 상품 영어 명
	private Integer unitprice; // 판매가
	private String pid; // 상품코드
	private String man; // 제조사
	private Integer unitcoupon; // 사용쿠폰
	private String filename; // 파일명
	
	public Product() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Product(String pname, String pename) {

		this.pname = pname;
		this.pename = pename;
	}

	public String getPname() {
		return pname;
	}

	public void setPname(String pname) {
		this.pname = pname;
	}

	public String getPename() {
		return pename;
	}

	public void setPename(String pename) {
		this.pename = pename;
	}

	public Integer getUnitprice() {
		return unitprice;
	}

	public void setUnitprice(Integer unitprice) {
		this.unitprice = unitprice;
	}

	public String getPid() {
		return pid;
	}

	public void setPid(String pid) {
		this.pid = pid;
	}

	public String getMan() {
		return man;
	}

	public void setMan(String man) {
		this.man = man;
	}

	public Integer getUnitcoupon() {
		return unitcoupon;
	}

	public void setUnitcoupon(Integer unitcoupon) {
		this.unitcoupon = unitcoupon;
	}
	
	
	
	

}
